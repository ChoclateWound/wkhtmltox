{
  "name": "wkhtmltox",
  "version": "0.0.1",
  "description": "high performance access to `wkhtmltopdf` and `wkhtmltoimage` from node.js.",
  "main": "index.js",
  "scripts": {
    "pretest": "jshint lib test ./index.js",
    "test": "mocha -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tcort/wkhtmltox.git"
  },
  "keywords": [
    "wkhtmltox",
    "wkhtmltopdf",
    "html",
    "pdf",
    "conversion"
  ],
  "author": "Thomas Cort <linuxgeek@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tcort/wkhtmltox/issues"
  },
  "homepage": "https://github.com/tcort/wkhtmltox",
  "readmeFilename": "README.md",
  "dependencies": {
    "slang": "0.3.x"
  },
  "devDependencies": {
    "jshint": "2.5.x",
    "expect.js": "0.3.x",
    "mocha": "1.19.x"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "jshintConfig": {
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "globalstrict": true,
    "immed": true,
    "indent": 4,
    "moz": true,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "trailing": true,
    "undef": true,
    "smarttabs": true,
    "strict": true,
    "validthis": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  }
}
